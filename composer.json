{
    "name": "arthaeus/mooshka",
    "description": "grabs data from govt data sites (csv,xml,whatever) and produces sql to populate a database",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": ["gov data"],
    "authors": [
        {
            "name": "Chris Davis",
            "email": "arthaeus@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "4.2.*",
        "pda/pheanstalk": "^3.1",
        "php-curl-class/php-curl-class": "*",
        "doctrine/dbal": "^2.5",
        "symfony/dependency-injection": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "Mooshka\\": "src/"
        }
    }
}
